@model IEnumerable<CapitalEuropeiaGuarda.Models.Empresaaluguer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Criar novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Morada)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Url)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Morada)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.empresaaluguerId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.empresaaluguerId">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.empresaaluguerId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
